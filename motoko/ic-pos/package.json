{
  "name": "ic-pos",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "description": "Accept Bitcoin payments (ckBTC) on the Internet Computer using this simple Point of Sale (POS) application.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kristoferlund/ic-pos"
  },
  "bugs": "https://github.com/kristoferlund/ic-pos/issues",
